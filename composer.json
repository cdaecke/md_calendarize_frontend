{
    "name": "mediadreams/md_calendarize_frontend",
    "type": "typo3-cms-extension",
    "description": "This TYPO3 extension enables frontend users to add ext:calendarize items in the frontend.",
	"license": "GPL-2.0-or-later",
    "keywords": [
        "TYPO3",
        "extension",
        "cal",
        "calendar",
        "calendarize",
        "frontend",
        "fe_users"
    ],
    "homepage": "http://www.mediadreams.org",
    "authors": [
        {
            "name": "Christoph Daecke",
            "role": "Developer",
            "homepage": "http://www.mediadreams.org"
        }
    ],
    "support": {
        "issues": "https://github.com/cdaecke/md_calendarize_frontend/issues",
        "source": "https://github.com/cdaecke/md_calendarize_frontend"
    },
    "require": {
		"typo3/cms-core": "^13.4",
		"lochmueller/calendarize": "^14.0",
		"georgringer/numbered-pagination": ">=1.0.1,<3.0.0"
    },
    "autoload": {
        "psr-4": {
            "Mediadreams\\MdCalendarizeFrontend\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "md_calendarize_frontend"
        }
   }
}
